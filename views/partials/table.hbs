<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
{{!-- Create a table --}}
<table border="1", cellpadding="5">
    <thead>
        <tr>
            {{#each data.[0]}}
            <th>
                {{@key}}
            </th>
            {{/each}}
        </tr>
    </thead>

    {{!-- Body section --}}
    <tbody>
        {{#each data}}
        <tr>
            {{#each this}}
            <td>{{this}}</td>
            {{/each}}
        <td><a href="#">Update</a></td>
        <td><a onclick="deleteCar({{this.id}})" href="#">Delete</a></td>
        </tr>
        {{/each}}
    </tbody>
</table>
<script src="./js/delete_car.js"></script>

